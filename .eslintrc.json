{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },

    "extends":["eslint: recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    
    "rules": {
        // "semi": ["error", "never"],
        "prettier/prettier": [
            "error",
        {
            "endOfLine": "auto"
        }
    ],
        // "camelcase": ["error"],
        "eqeqeq": ["error", "always"]
    }
}